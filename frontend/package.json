    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix"
  },
  "//": "ESLintのrulesについてはプロジェクトごとに設定してください（不要の場合は削除してください）",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": [
      "*.css"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "jest-junit": "^12.0.0"
  },
  "jest": {
    "globalSetup": "./setup.js"
  }
